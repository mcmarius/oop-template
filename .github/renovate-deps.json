{
  "recreateWhen": "always",
  "enabledManagers": ["regex"],
  "baseBranches": [
    "main",
    "tests/gtest",
    "baze-de-date/pqxx"
  ],
  "customManagers": [
    {
      "customType": "regex",
      "managerFilePatterns": [
        ".github/workflows/*.yml",
        ".github/actions/**/*.yml",
        "CMakeLists.txt",
        "README.md",
        "tests/CMakeLists.txt",
        "scripts/*.sh"
      ],
      "matchStrings": [
        "# renovate: datasource=(?<datasource>\\S+?) depName=(?<depName>\\S+?) versioning=(?<versioning>\\S+?)\\s+.*=(?<currentValue>\\S+)",
        "# renovate: datasource=(?<datasource>\\S+?) depName=(?<depName>\\S+?) versioning=(?<versioning>\\S+?)\\s+.*: \"(?<currentValue>\\S+)\"",
        "# renovate: datasource=(?<datasource>\\S+?) depName=(?<depName>\\S+?) versioning=(?<versioning>\\S+?)\\s+GIT_TAG (?<currentValue>\\S+)",
        "<!--\\s*renovate:\\s*datasource=(?<datasource>\\S+)\\s+depName=(?<depName>\\S+)\\s+versioning=(?<versioning>\\S+)\\s*-->\\s*-\\s*Versiunea\\s+folositÄƒ:\\s*\\[v(?<currentValue>[^\\]]+)\\]\\([^\\)]+\\)"
      ],
      "datasourceTemplate": "{{{datasource}}}",
      "depNameTemplate": "{{{depName}}}",
      "versioningTemplate": "{{#if versioning}}{{{versioning}}}{{else}}semver-coerced{{/if}}"
    }
  ],
  "prConcurrentLimit": 0,
  "prHourlyLimit": 0
}
